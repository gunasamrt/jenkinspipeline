pipeline {
    agent any

    stages {
        stage('BUild') {
            steps {
                echo 'Hello I BUild'
            }
        }
        stage('TEST') {
            steps {
                echo 'Hello TEST'
            }
        }
        stage('DEPLOY') {
            steps {
                echo 'Hello Deploying'
            }
        }
        stage('Monitor') {
            steps {
                echo 'Hello I am Monitor'
            }
        }
        stage('PROD') {
            steps {
                echo 'Hello I Production'
            }
        }
    }
}
